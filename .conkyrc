background yes
use_xft yes
xftfont HandelGotD:size=9
#xftfont NeutraTextTF:size=8
xftalpha 0.5
update_interval 4.0
imlib_cache_size 0  
total_run_times 0
own_window yes
own_window_type normal
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
minimum_size 200 700
maximum_width 220
draw_shades no
draw_outline no
draw_borders no
draw_graph_borders no
default_color D6D6D6
defau0lt_shade_color E8E8E8
default_outline_color EBA4A4
alignment top_right
gap_x 12
gap_y 48
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale no
TEXT
$alignc ${color red}Ubuntu${color D6D6D6}
Uptime $alignr $uptime
Load $alignr $loadavg

#${color 5C67FF}IP $alignr ${execi 3 wget http://automation.whatismyip.com/n09230945.asp -O - -q}${color D6D6D6}
#Inbound $alignr ${downspeed wlan0} kb/s
#${downspeedgraph wlan0}
#Outbound $alignr ${upspeed wlan0} kb/s
#${upspeedgraph wlan0}
$processes processes ($running_processes running)
CPU1 $alignr ${cpu cpu0}%
${cpubar cpu0}
CPU2 $alignr ${cpu cpu1}%
${cpubar cpu1}
MEMORY $alignc $mem / $memmax $alignr $memperc%
$membar
/home $alignc ${fs_used /home} / ${fs_size /home} $alignr ${fs_free_perc /home}%
${fs_bar /home}
${color red}PROCESS $alignr PID    CPU${color D6D6D6}
${top name 1} $alignr ${top pid 1} ${top cpu 1}
${top name 2} $alignr ${top pid 2} ${top cpu 2}
${top name 3} $alignr ${top pid 3} ${top cpu 3}
${top name 4} $alignr ${top pid 4} ${top cpu 4}
${top name 5} $alignr ${top pid 5} ${top cpu 5}
${top name 6} $alignr ${top pid 6} ${top cpu 6}
${top name 7} $alignr ${top pid 7} ${top cpu 7}
${top name 8} $alignr ${top pid 8} ${top cpu 8}

$alignr${color red}WEATHER${color D6D6D6}
$alignr${font ConkyWeather:size=44}${execi 3 conkyForecast --location=USNY1232 --datatype=WF -i}              ${font Bauhaus:size=9}
${execi 3 conkyForecast --location=USNY1232 --datatype=OB --hideunits}
Current: ${execi 3 conkyForecast --location=USNY1232 --datatype=CC --hideunits -L en}
Temperature: ${execi 3 conkyForecast --location=USNY1232 --datatype=HT -L en --hideunits -x} F
Humidity: ${execi 3 conkyForecast --location=USNY1232 --datatype=HM --hideunits}
Moon Phase: ${execi 3 conkyForecast --location=USNY1232 --datatype=MP -L en -i}

$alignr${color red}NOW PLAYING${color D6D6D6}
${font size=9}${execi 3 /home/alkaline/musictitle.sh}




${execi 3 /home/alkaline/musicart.sh}
${image /home/alkaline/this.jpg -s 100x100 -p 0,600}
